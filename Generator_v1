//13 objects and each one hold the specs that it can be   
const randomMainCharacter = mainCharacter[Math.floor(Math.random() * mainCharacter.length)]; const myRace = randomMainCharacter.mainRace; const myClass = randomMainCharacter.mainClass[Math.floor(Math.random() * randomMainCharacter.mainClass.length)]; const myMainClass = myClass.name; const myMainSpec = myClass.specs[Math.floor(Math.random() * myClass.specs.length)]; let mainSpec: string[] = [] //Random Name Generator 
const consonantLetters = ["b", "c", "d", "f", "g", "h", "j", "k", "l", "m", "n", "p", "q", "r", "s", "t", "v", "w", "x", "y", "z"];
const vowelLetters = ["a", "e", "i", "o", "u"]
let randomName = [];
for (let i = 0; i < 6; i++) {
    if (i == 1 || i == 4) { randomName.push(vowelLetters[Math.floor(Math.random() * vowelLetters.length)]) }
    else {
        let letter = consonantLetters[Math.floor(Math.random() * consonantLetters.length)];
        if (i == 0) letter = letter.toUpperCase(); randomName.push(letter)
    }
}
console.log("Your main character is " + randomName.join("") + " the " + myRace + " " + myMainSpec + " " + myMainClass);
